{
  "name": "frontend-p2-scalable",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "npx tailwindcss -i ./src/input.css -o ./dist/output.css",
    "dev:css": "npx tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "dev:vite": "vite",
    "dev": "run-p dev:*",
    "build": "vue-tsc --noEmit && npm run predev && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@videojs-player/vue": "^1.0.0",
    "axios": "^1.5.1",
    "axios-auth-refresh": "^3.3.6",
    "core-js": "^3.29.0",
    "lodash.debounce": "^4.0.8",
    "npm-run-all": "^4.1.5",
    "roboto-fontface": "*",
    "run-p": "^0.0.0",
    "socket.io-client": "^4.7.2",
    "v3-infinite-loading": "^1.3.1",
    "vue": "^3.2.0",
    "vue-axios": "^3.5.2",
    "vue-loading-overlay": "^6.0.3",
    "vue-observe-visibility": "^2.0.0-alpha.1",
    "vue-router": "^4.0.0",
    "vue3-smooth-scroll": "^0.8.1",
    "vuetify": "^3.0.0",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@types/node": "^18.18.5",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": "^8.0.0",
    "sass": "^1.60.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.0",
    "unplugin-fonts": "^1.0.3",
    "vite": "^4.2.0",
    "vite-plugin-require-transform": "^1.0.21",
    "vite-plugin-vuetify": "^1.0.0",
    "vue-tsc": "^1.2.0"
  }
}
